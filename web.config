<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />
        <defaultDocument>
            <files>
                <clear />
				<add value="index.php" />    
				<add value="Default.htm" />    
				<add value="index.htm" />    
				<add value="index.html" />   
				<add value="iisstart.htm" />
				<add value="default.aspx" />
            </files>
        </defaultDocument>
        <handlers>
            <remove name="php" />
            <add name="php" path="*.php" verb="*" modules="IsapiModule" scriptProcessor="D:\php\gmodule-2.dll" resourceType="Unspecified" requireAccess="Script" preCondition="bitness64" />
        </handlers>
    </system.webServer>
</configuration>
